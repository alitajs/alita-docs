{
  "name": "next",
  "scope": "alitajs",
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@now/static-build",
      "config": {
        "distDir": "alita-docs"
      }
    }
  ],
  "routes": [{
    "src": "/(.*)",
    "dest": "/doc_dist/$1"
  }, {
    "src": "/(.*)",
    "dest": "/doc_dist/404.html"
  }],
  "alias": ["next.alitajs.com"]
}
